.uploader{
  margin-top: 40rpx;
  text-align: center;
  height: 140rpx;
  width: 100%;
  background: #fff;
  border: 1px solid rgba(0, 0, 0, 0.1);
  border-left: none;
  border-right: none;
  display: flex;
  flex-direction: row;
  align-items: center;
  transition: all 300ms ease;
}
.uploader-text{
  width: 100%;
  line-height: 52px;
  font-size: 40rpx;
  color: #007aff;
}